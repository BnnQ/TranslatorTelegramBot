{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "UsersDatabase": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=TranslatorBotUsers;Integrated Security=True;"
 },
  "BotConfiguration": {
    "BotToken": "<BotToken>",
    "Route": "/bot",
    "HostAddress": "<HostAddress>",
    "SecretToken": "<SecretToken>"
  },
  "Azure": {
    "Translator": {
      "Endpoint": "https://api.cognitive.microsofttranslator.com/",
      "SubscriptionKey": "<SubscriptionKey>",
      "Region": "<SubscriptionRegion>"
    }
  }
}
